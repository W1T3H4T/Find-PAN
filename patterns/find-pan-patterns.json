{
  "Anti-PAN Pattern": {
    "Sequential Numbers": {
      "regex": "(123456|654321)[0-9]{12,19}",
      "length": "19"
    },
    "Repeated Numbers": {
      "regex": "([3456]\\d{3,5})\\1+",
      "length": 19
    },
    "Repeated Numbers Extended": {
      "regex": "[34356](\\d)\\1{2}(([0-9](\\d)\\1{2}){2})",
      "length": 19
    },
    "654321 with Trailing Zeros": {
      "regex": "(?=.{12,19}$)6?5?4321[0]+",
      "length": 19
    }
  },

  "TRACK Pattern": {
    "Track 1": {
      "regex": "%[BM][0-9]{12,19}[\\^][A-Za-z0-9/,]+[\\^][0-9]+[\\?]",
      "length": "12-24"
    },
    "Track 2": {
      "regex": ";[0-9]{12,19}=[0-9]+[?]",
      "length": "12-24"
    }
  },

  "PAN Pattern": {
    "American Express": {
      "regex": [ "(34|37)[0-9]{13}" ],
      "length": "15"
    },
    "Diners Club Carte Blanche": {
      "regex": [ 
            "(301|302|303|304|305)[0-9]{11}",
            "(300[0-5])[0-9]{10}" ], 
      "length": "14"
    },
    "Diners Club International": {
      "regex": [ "(36)[0-9]{12}" ],
      "length": "14"
    },
    "Diners Club US and Canada": {
      "regex": [ "(54|55)[0-9]{14}" ],
      "length": "16"
    },
    "Discover Card": {
      "regex": [ 
          "(62212[6-9]|62292[3-5])[0-9]{10}", 
          "(6221[3-9]|6229[0-2])[0-9]{11}", 
          "(6011)[0-9]{12}", 
          "(622[2-8]|64[4-9])[0-9]{13}", 
          "(65)[0-9]{14}" ],
      "length": "16"
    },
    "InstaPayment": {
      "regex": [ "(637|638|639)[0-9]{13}" ], 
      "length": "16"
    },
    "JCB": {
      "regex": [ "(3528|3529|353|354|355|356|357|358|359)[0-9]{12}" ],
      "length": "16"
    },
    "Laser": {
      "regex": [ "(6304|670[69]|6771|6709)[0-9]{10,13}" ],
      "length": "16-19"
    },
    "Maestro": {
      "regex": [ "(5018|5020|5038|6304|6759|6761|6762|6763)[0-9]{8,15}" ] ,
      "length": "12-19"
    },
    "Mastercard": {
      "regex": [ "(51|52|53|54|55)[0-9]{14}" ],
      "length": "16"
    },
    "VISA": {
      "regex": [ "(4)[0-9]{12,15}" ],
      "length": "13-16"
    },
    "VISA Electron": {
      "regex": [ 
            "(4026|4508|4844|4913|4917)[0-9]{12}", 
            "(417500)[0-9]{10}" ],
      "length": "16"
    }
  }
}

